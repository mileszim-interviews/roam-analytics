<div class="container">
  <div class="inner-container">
    {{input type="text" value=stateFilter}}

    <table>
      <tbody>
        {{#each filteredStates as |stateObj|}}
        <tr>
          <td>{{stateObj.state}}</td>
          <td>{{stateObj.count}}</td>
        </tr>
        {{/each}}
        <tr class="total">
          <td><strong>Total</strong></td>
          <td>{{total}}</td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
